/* bq OS
---------------------------------*/

*{
background:none;
color:#000000;
font-size:16px;
font-family: 'Lato', Arial, Helvetica, sans-serif;
}
QWidget#bqViewerCont {
background-color: #FFFFFF;
font-family: 'Lato',Arial, Helvetica, sans-serif;
}
QPushButton{
outline:0px;
border:none;
font-weight:bold;
}
QHBoxLayout, QVBoxLayout{
margin:0px;
padding:0px;
}
QFrame {
border: none;
}
QLabel {
font-family:'Lato',Arial, Helvetica, sans-serif;
}
QLineEdit{
min-height:35px;
max-height:35px;
padding:0px 15px;
color:#000000;
font-size:18px;
border-style: solid;
background-color:#FFFFFF;
border: 1px solid #000000;
border-radius: 6px;
font-family:'melainvento';
}

QTextEdit{
padding:15px;
background-color:transparent;
font-family:'melainvento';
}
QTextBrowser, bqQTextBrowser{
font-size:19px;
font-family: 'Lato', Arial, Helvetica, sans-serif;
}

MyQLineEdit{
font-family:'melainvento';
}

MyQTextEdit{
font-family:'melainvento';
}

QRadioButton,QCheckBox{
outline:none;
color:#000000;
}
QRadioButton::indicator,QCheckBox::indicator {
width: 37px;
height: 37px;
}
QCheckBox::indicator:unchecked {
border:1px solid #000000;
border-radius: 4px;
}
QCheckBox::indicator:unchecked:pressed {
border:1px solid #000000;
border-radius: 4px;
}
QCheckBox::indicator:unchecked:pressed {
border:1px solid #000000;
border-radius: 4px;
}
QCheckBox::indicator:checked {
border:1px solid #000000;
border-radius: 4px;
image: url(:/res/elf_checked.png);
}
QCheckBox::indicator:checked:pressed {
border:1px solid #000000;
border-radius: 4px;
image: url(:/res/elf_checked.png);
}
QCheckBox::indicator:checked:pressed {
border:1px solid #000000;
border-radius: 4px;
image: url(:/res/elf_checked.png);
}
QCheckBox::indicator:indeterminate:pressed {
border:1px solid #000000;
border-radius: 4px;
image: url(:/res/elf_checked.png);
}
QCheckBox::indicator:indeterminate:pressed {
border:1px solid #000000;
border-radius: 4px;
image: url(:/res/elf_checked.png);
}
#checkBtn{
width: 37px;
height: 37px;
border:1px solid #000000;
border-radius: 4px;
}
QComboBox{
padding:0px 15px;
min-height:35px;
max-height:35px;
color:#000000;
border-style: solid;
background-color:#FFFFFF;
border: 1px solid #000000;
border-radius: 6px;
}
QComboBox::drop-down{
min-width:25px;
max-width:25px;
background-repeat:no-repeat;
background-position:left;
background-image:  url(:/res/bq/combo_arrow.png);
}
QComboBox QAbstractItemView{
background-color:#FFFFFF;
border: 1px solid #000000;
}


/*******Barras de progreso*******/
QProgressBar{
min-height:8px;
max-height:8px;
background-color:#FFFFFF;
border: 1px solid #000000;
border-radius: 4px;
}
QProgressBar:chunk{
color:#FFFFFF;
background-color:#000000;
}
#content, #content2{
background-color:#FFFFFF;
}
/* Overlay
---------------------*/
#shadow, #ElfAppMenu, #whiteShadow{
background-color:transparent;
background-color: rgba(255, 255, 255, 255);
}
#whiteCont{
background-color:#FFFFFF;
}

/*BookMenu
----------------------------*/
#bookMenuCont{
background-color:#FFFFFF;
border:2px solid #000000;
border-radius: 4;
}
#vline_1,#vline_2,#vline_3,#vline_4,#vline_5,#vline_6{
