color:#000000;
}
#tabCont #on{
border: 2px solid #000000;
background-color:#FFFFFF;
border-top-left-radius:10px;
border-top-right-radius:10px;
border-bottom:none;
}


/*Keyboard
-------------------------------------*/
#keyboardCont {
