/*Progress Bar
---------------------*/
QProgressBar{
min-height:8px;
max-height:8px;
border-width:1px;
border-radius: 4px;
}
QProgressBar:chunk{
}
QSlider{
}
QSlider::groove:horizontal {
/*border-width: 1px;
border-radius: 4px;*/
height: 10px;
}
QSlider::sub-page:horizontal {
margin-left:10px;
border-width: 1px;
height: 10px;
border-top-left-radius: 4px;
border-bottom-left-radius: 4px;
}
QSlider::add-page:horizontal {
margin-right:10px;
border-width: 1px;
height: 10px;
border-top-right-radius: 4px;
border-bottom-right-radius: 4px;
}
QSlider::handle:horizontal {
min-width:50px;
max-width:50px;
min-height:50px;
max-height:50px;
width:50px;
height:50px;
margin-top: -22px;
margin-bottom: -20px;
border-radius: 4px;
}
QSlider::handle:horizontal:hover {
}
QSlider::sub-page:horizontal:disabled {
}
QSlider::add-page:horizontal:disabled {
}
QSlider::handle:horizontal:disabled {
border-width: 1px;
border-radius: 4px;
}

